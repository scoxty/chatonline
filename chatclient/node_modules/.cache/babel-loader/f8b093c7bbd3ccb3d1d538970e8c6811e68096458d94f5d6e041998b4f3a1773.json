request.post(\"/hy/update\", updatedFriend).then(res => {\r\n                if (res.code === '0') {\r\n                    this.$message({\r\n                        message: '分组更新成功',\r\n                        type: 'success'\r\n                    });\r\n                    // 更新前端显示的分组名称\r\n                    const index = this.tableData.findIndex(friend => friend.id === this.currentFriend.id);\r\n                    if (index !== -1) {\r\n                        this.tableData[index].hyGroupName = this.newGroupName;\r\n                    }\r\n                    this.moveGroupDialogVisible = false;\r\n                } else {\r\n                    this.$message({\r\n                        message: res.msg,\r\n                        type: 'error'\r\n                    });\r\n                }\r\n            });\r\n        }\r\n    }\r\n}\r\n</script>\r\n"],"mappings":"AAgDA,OAAAA,OAAA;AAEA;EACAC,KAAA;IACA;MACAC,MAAA;QACAC,OAAA;QACAC,QAAA;MACA;MACAC,IAAA,EAAAC,YAAA,CAAAC,OAAA,WAAAC,IAAA,CAAAC,KAAA,CAAAH,YAAA,CAAAC,OAAA;MACAG,SAAA;MACAC,KAAA;MACAC,iBAAA;MACAC,IAAA;MACAC,sBAAA;MACAC,aAAA;MACAC,YAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,YAAA;EACA;EACAC,OAAA;IACAD,aAAA;MACAlB,OAAA,CAAAoB,GAAA;QACAlB,MAAA,OAAAA;MACA,GAAAmB,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAb,SAAA,GAAAY,GAAA,CAAArB,IAAA,CAAAuB,IAAA;UACA,KAAAb,KAAA,GAAAW,GAAA,CAAArB,IAAA,CAAAU,KAAA;QACA;UACA,KAAAc,QAAA;YACAC,OAAA,EAAAJ,GAAA,CAAAK,GAAA;YACAC,IAAA;UACA;QACA;MACA;IACA;IACAC,iBAAAzB,QAAA;MACA,KAAAF,MAAA,CAAAE,QAAA,GAAAA,QAAA;MACA,KAAAc,YAAA;IACA;IACAY,oBAAA3B,OAAA;MACA,KAAAD,MAAA,CAAAC,OAAA,GAAAA,OAAA;MACA,KAAAe,YAAA;IACA;IACAa,IAAAC,EAAA;MACAhC,OAAA,CAAAiC,MAAA,UAAAD,EAAA,EAAAX,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAE,QAAA;YACAC,OAAA;YACAE,IAAA;UACA;UACA,KAAAV,YAAA;QACA;UACA,KAAAO,QAAA;YACAC,OAAA,EAAAJ,GAAA,CAAAK,GAAA;YACAC,IAAA;UACA;QACA;MACA;IACA;IACAM,oBAAAC,MAAA;MACA,KAAApB,aAAA;QAAA,GAAAoB;MAAA;MACA,KAAAnB,YAAA,GAAAmB,MAAA,CAAAC,WAAA;MACA,KAAAtB,sBAAA;IACA;IACAuB,YAAA;M